#include <iostream>
#include <algorithm>

int main() {
    int arr[] = {1, 2, 3, 1, 2, 4, 1, 3, 5, 4};
    int size = sizeof(arr) / sizeof(arr[0]);

    // Sort the array
    std::sort(arr, arr + size);

    // Display the sorted array
    std::cout << "Sorted Array: ";
    for (int i = 0; i < size; ++i) {
        std::cout << arr[i] << " ";
    }

    return 0;
}
